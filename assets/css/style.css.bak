*{
	padding:0px;
	margin:0px;
	box-sizing:border-box;
}
a:link,
a:hover,
a:focus,
a:active{
	text-decoration:none;
}
.text-center{
	text-align:center;
}
.container{
	width:1170px;
	margin:0 auto;
}
.bg-color{
	background-color:#f5f7fa;
}
.row,
.row::after,
.row::before{
	clear:both;
	display:table;
	width:100%;
}
.col6, .col2, .col8, .col12, .col4, .col3, .col9{
	float:left;
	padding:0px 15px;
}
.col3{
	width:25%;
}
.col9{
	width:75%;
}
.col6{
	width:50%;
}
.col2{
	width:20%;
}
.col12{
	width:100%;
}
.col8{
	width:60%;
}
.col6 ul{
	list-style:none;
	font-size:12px;
}
.col10{
	width:calc((100% * 10) / 12);
}
.col6 ul li {
	display:inline-block;
}
.col4{
	width:calc(100% / 3);
}
.mb-20{
	 margin-bottom:20px;
}
.margin, .para-2{
	padding:2.5px;
}
.info {
	padding-top:10px;
}
.info a{
	padding:15px 15px;
}
i{
	padding:5px;
}
.image{
	width:100%;
	padding:0 15px;
}
.h-icon{
		font-size:12px;
		float:right;
		display:inline-block;
		padding:10px;
}
.logo{
	padding:35px;
}
.navbar li{
	list-style:none;
	display:inline-block;
	padding:50px 22px;
	font-size:15px;
}
.search{
	float:right;
	padding:50px 22px;
}
.heroimage{
	width:100%;
	height:100vh;
	background-image:url(../images/untitled-9.jpg);
	background-size:cover;
	background-repeat:no-repeat;
	background-position:center center;
}
.button{
	text-align:center;
	/* background-color:; */
}

.heading{
	font-size:78px;
	margin:0 auto;
	text-align:center;
	color:white;
}
.para{
	margin-top:30px;
	margin-bottom:60px;
}
.image img{
	width:100%;
}
.caption{
	padding:150px;
	color:white;
}
.beautiful, .checkOut, .work{
	padding:130px 0px;
}
.btn{
	padding:25px 70px;
}
.btn-2{
	padding:17px 20px;
}
.btn-3{
	padding:10px 5px;
	font-size:13px;
	border-radius:5px;
}
.btn, .btn-2{
	font-size:18px;
}
.btn, .btn-2, .btn-3{
	cursor:pointer;
	background-color:#00bc9c;
	color:#ffffff;
	border:2px;
	outline:none;
}
.btn:active,
.btn:focus,
.btn:hover,
.btn:visited{
	color:#00bc9c;
	background-color:#fff;
	border-color:#00bc9c;
}
.btn-2:active,
.btn-2:focus,
.btn-2:hover,
.btn-2:visited{
	color:#00bc9c;
	background-color:#fff;
	border:2px solid #00bc9c;
}
.sec-heading{
	font-size:44px;
	text-align:center;
	color:#5f768e;
}
.sec-para{
	font-size:22px;
	margin-bottom:80px;
	margin-top:32px;
	color:#93a1b1;
	text-align:center;
}
.thumb-img i {
	font-size:70px;
	color:#00bc9c;
}
.thumb-info h3{
	font-size:26px;
	margin-bottom:30px;
	color:#506a85;
}
.thumb-info p{
	color:#73879c;
	font-size:14px;
	line-height:15px;
}
.head{
	padding:30px;
	margin-bottom:15px;
}
.head-h5{
	font-size:20px;
	color:#506a85;
	margin-bottom:15px;
}
.head-h3{
	color:#73879c;
}
.black{
	color:black;
}
.reen{
	padding:75px 0px;
	background-color:#2f4052;
}
.r-head{
	font-size:16px;
	color:#a3b1bf;
	padding:27px 0px;
}
.para-1, .para-2, .f-color, .f-head, .nav, .info-1 li a {
	color:#a3b1bf;
}
.para-1{
	line-height:17px;
}
.sec-img{
	width:100%;
}
.pd-25{
	padding:25px 0px;
}
.info-1 li{
	list-style:none;
}
.footer{
	padding:35px 0px;
	background-color:#283645;
}
.nav li {
	padding:12px;
}
.nav li, .f-head, .para-1, .para-2, .head-h3, .email{
	font-size:14px;
}
.mg-btn{
	background-color:#4e5c6c;
	border-radius:5px;
	padding:2px;
}
.email{
	outline:none;
	border:0px;
}